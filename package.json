{
  "name": "lanegap",
  "version": "1.0.0",
  "private": true,
  "description": "League of Legends Midlane Coaching App - Think like a pro player building his own tool",
  "author": "LaneGap Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:full": "concurrently -n \"next,pb\" -c \"cyan,magenta\" \"bun dev\" \"cd pocketbase && ./pocketbase serve\"",
    "pb": "cd pocketbase && ./pocketbase serve",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "sync:champions": "bun run scripts/sync-champions.ts",
    "sync:items": "bun run scripts/sync-items.ts",
    "seed:champions": "bun run scripts/seed-champions.ts",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "clean": "rm -rf .next node_modules"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tanstack/react-query": "^5.90.12",
    "clsx": "^2.1.1",
    "input-otp": "^1.4.2",
    "next": "16.0.10",
    "next-intl": "^4.6.0",
    "pocketbase": "^0.26.5",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "react-icons": "^5.5.0",
    "resend": "^6.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "zod": "^4.2.0"
  }
}
